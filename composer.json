{
    "name": "icy2003/icyii2003",
    "description": "icy2003's Yii2 project",
    "keywords": [
        "yii2",
        "framework",
        "advanced",
        "project"
    ],
    "homepage": "https://www.icy2003.com",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/icy2003/icyii2003/issues",
        "wiki": "https://github.com/icy2003/icyii2003/wiki",
        "source": "https://github.com/icy2003/icyii2003"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yidas/yii2-bower-asset": "~2.0.5",
        "yiisoft/yii2": "~2.0.14",
        "icy2003/php": "dev-dev",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ]
}